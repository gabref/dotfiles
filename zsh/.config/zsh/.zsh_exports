# exports

# XDG Paths
export XDG_CONFIG_HOME=$HOME/.config
export XDG_CACHE_HOME=$HOME/.cache
export XDG_DATA_HOME=$HOME/.local/share

export PATH=$HOME/.local/bin:$PATH
export PATH=$HOME/bin:/usr/local/bin:$PATH

# add tmux t script to the path
export PATH=$XDG_CONFIG_HOME/zsh/my_scripts:$PATH

# Path to your oh-my-zsh installation.
export ZSH="$HOME/.oh-my-zsh"
export ZSH_COMPDUMP=$HOME/.cache/.zcompdump-$HOST-$ZSH_VERSION

# Added by n-install (see http://git.io/n-install-repo)
export N_PREFIX="$HOME/n"; [[ :$PATH: == *":$N_PREFIX/bin:"* ]] || PATH+=":$N_PREFIX/bin"

# go
export PATH=$PATH:/usr/local/go/bin

export PATH=/mnt/c/Windows/system32:/mnt/c/Windows:/mnt/c/Windows/System32/Wbem:$PATH
